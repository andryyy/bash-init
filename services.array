stress_ng=(
  [system_packages]=stress-ng,procps,pstree
  [command]="stress-ng --vm 1 --vm-bytes 512M --vm-hang 0"
  [restart]="on-failure"
  [stop_signal]=SIGQUIT,15
  [probe]="http:debinux.de:80:/:GET:301"
  [success_exit]=0
  [probe_retries]=3
)

python=(
  [system_packages]=python3
  [command]="python3 -m http.server 8000"
  [restart]="on-failure"
  [stop_signal]=SIGTERM
  [probe]="http:example.com:81:/:GET:200"
  [success_exit]=0
  [probe_retries]=1
)
