stress_ng=(
  [system_packages]=stress-ng,procps,pstree
  [command]="stress-ng --vm 2 --vm-bytes 512M --vm-hang 0"
  [restart]="never"
  [stop_signal]=SIGTERM
  [probe]="http:example.com:80:/:GET:200"
  [success_exit]=1
)

python=(
  [system_packages]=python3
  [command]="python3 -m http.server 8000"
  [restart]="on-failure"
  [stop_signal]=SIGTERM
  [probe]="http:example.com:80:/:GET:200"
  [success_exit]=0
)
